    <script type="text/javascript">

        $(function () {

            $("#button").hide();
            $("#mark").change(function () {
                $("#button").hide();
                $('#engines').find('option').remove().end().append('<option  selected=""> - Silnik - </option>').val('Silnik');
                var url = '@Url.Content("~/")' + 'Products/ModelsList';
                var ddlsource = "#mark";

                $.getJSON(url,{ id: $('#mark').val() }, function (data) {
                    var items = ' ';
                    $("#seriesA").empty();
                    $.each(data, function (i, row) {
                        items += "<option value='" + row.value + "'>" + row.text + "</option>";
                    });
                    $('#seriesA').html(items);
                })
            });

            $("#seriesA").change(function () {
                $("#button").hide();
                var url = '@Url.Content("~/")' + 'Products/PcList';
                var ddlsource = "#seriesA";

                $.getJSON(url, { id: $('#seriesA').val() }, function (data) {
                    var items = ' ';
                    $("#engines").empty();
                    $.each(data, function (i, row) {
                        items += "<option value='" + row.value + "'>" + row.text + "</option>";
                    });
                    $('#engines').html(items);
                })
            });
            $("#engines").change(function () {
                $("#button").show();
            });

        });
