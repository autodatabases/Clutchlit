    <script type="text/javascript">

        function CheckEngine() {
            var ktype = 0;
            ktype = $('#engines').val();
            if (ktype == null)
                ktype = 0;
            return ktype;
        }
        function CheckcheckedVals() {
            var checkedVals = $('.category:checkbox:checked').map(function () {
                return this.value;
            }).get();
            return checkedVals;
        }

        $(document).ready(function () {

            $('#getoffers').submit(function (event) {

                event.preventDefault();
                var table = $('#example3').DataTable({
                    "destroy": true,
                    "processing": false, // for show progress bar
                    "serverSide": false, // for process server side
                    "filter": true, // this is for disable filter (search box)
                    "orderMulti": false, // for disable multiple column at once
                    "ajax": {
                        "url": "/AllegroAuctions/OffersList",
                        "type": "POST",
                        "datatype": "json",
                        "data": ""
                    },
                    "columnDefs":
                        [{
                            "targets": [0],
                            "visible": true,
                            "searchable": false,
                            "checkboxes": {
                                "selectRow": true
                            }
                        }],
                    "select": {
                        "style": "multi"
                    },
                    "columns": [
                        { "data": "id", "name": "Id" },
                        { "data": "name", "name": "Name" },
                        { "data": "category", "name": "Category" },
                        { "data": "primaryImage", "name": "PrimaryImage", "render": function (data, type, row) { return "<img height='100px' src='" + data + "' />"; } },
                        { "data": "price", "name": "Price" },
                        { "data": "watchers", "name": "watchers" },
                        { "data": "visits", "name": "visits" },
                        { "data": "status", "name": "status" }
                    ],
                    "order": [[1, 'asc']],

                });

                $('#end_selected').click(function (event) {

                    if (confirm("Czy jesteś pewien, że chcesz wyłączyć zaznaczone aukcje?")) {
                        var idR;
                        var rows_selected = table.column(0).checkboxes.selected();
                        $.each(rows_selected, function (index, rowId) {
                            idR = rowId;
                            $.ajax({
                                type: "POST",
                                url: "/AllegroAuctions/EndOffer/" + idR + "/",
                                data: { "Id": idR },
                                contentType: "application/json;charset=utf-8",
                                dataType: "json",
                                success: function (msg) {
                                    alert(msg);
                                },
                                error: function (msg) {
                                    alert("Coś poszło nie tak. Zmiany nie zostały poczynione :(");
                                }
                            });
                        });


                    }
                    else {

                    }
                });
                $('#start_selected').click(function (event) {

                    if (confirm("Czy jesteś pewien, że chcesz wyłączyć zaznaczone aukcje?")) {
                        var idR;
                        var rows_selected = table.column(0).checkboxes.selected();
                        $.each(rows_selected, function (index, rowId) {
                            idR = rowId;
                            $.ajax({
                                type: "POST",
                                url: "/AllegroAuctions/ActivateOffer/" + idR + "/",
                                data: { "Id": idR },
                                contentType: "application/json;charset=utf-8",
                                dataType: "json",
                                success: function (msg) {
                                    alert(msg);
                                },
                                error: function (msg) {
                                    alert("Coś poszło nie tak. Zmiany nie zostały poczynione :(");
                                }
                            });
                        });


                    }
                    else {

                    }
                });

            });

            // Tabela z produktami
            var tableA = $('#example2').DataTable({
                "destroy": true,
                "processing": true, // for show progress bar
                "serverSide": true, // for process server side
                "filter": true, // this is for disable filter (search box)
                "orderMulti": false, // for disable multiple column at once
                "ajax": {
                    "url": "/Products/ProductsList",
                    "type": "POST",
                    "datatype": "json",
                    "data": function (d) {
                        d.engine = CheckEngine();
                        d.category_id = CheckcheckedVals();
                    }
                },
                "columnDefs":
                    [{
                        "targets": [0],
                        "visible": true,
                        "searchable": false
                    }],
                "columns": [
                    { "data": "id", "name": "Id", "width": "10%" },
                    { "data": "name", "name": "name", "width": "32%" },
                    { "data": "reference", "name": "reference", "width": "15%" },
                    { "data": "gross_price", "name": "gross_price", "width": "9%" },
                    { "data": "lowestPrice", "name": "lowest_price", "width": "9%" },
                    { "data": "distributorId", "name": "distributor_id", "width": "15%", "className": "distributors" },
                    { "data": "markup", "name": "markup", "width": "10%" },
                    {
                        "render": function (data, type, full, meta) { return '<a class="add_to_busket btn btn-default" name="' + full.id + '">Kup</a>'; }
                    },
                ],

                "order": [[1, 'asc']],

            });

            $("#example2 tbody").on('click', 'tr', function () {
                //alert("dd");
                var tr = $(this).closest('tr');

                var row = tableA.row(tr);
                if (row.child.isShown()) {
                    // This row is already open - close it
                    row.child.hide();
                    tr.removeClass('shown');
                }
                else {
                    // Open this row
                    //alert("da");
                    row.child(format(row.data())).show();
                    tr.addClass('shown');
                }
            });
            // click

            // Tabela z produktami

            $('#ktypes_search').submit(function (event) {
                event.preventDefault();
                $("#example2").DataTable().ajax.reload();
            });

        });

        $(document).on('click', '.add_to_busket', function (e) {
            e.preventDefault();
            // alert($(this).attr('name'));
            $.ajax({
                type: "POST",
                url: "/Products/AddToCart/" + $(this).attr('name') + "/",
                data: { "Id": $(this).attr('name') },
                contentType: "application/json;charset=utf-8",
                success: function (msg) {
                    alert(msg);
                },
                error: function (msg) {
                    alert("Nie udało się dodać produktu do koszyka");
                }
            });
        });


    </script>
